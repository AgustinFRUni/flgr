---
import MainGrid from '../../components/MainGrid.astro';
import Layout from '../../layouts/Layout.astro';

export async function getStaticPaths() {
  return [
    { params: { cat: 'icon' } },
    { params: { cat: 'sticker' } },
    { params: { cat: 'shape' } }
  ];
}

const { cat } = Astro.params;
---

<Layout title='Hola'>
  <MainGrid cat={cat} />
</Layout>